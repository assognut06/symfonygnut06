<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% include '_partials/header.html.twig' %}
		
	</head>
	<body>

		{% include '_partials/navbar.html.twig' %}
		{% include '_partials/main.html.twig' %}
		{% for flash_error in app.flashes('reset_password_error') %}
			<div class="alert alert-danger" role="alert">{{ flash_error }}</div>
		{% endfor %}
		{% block body %}{% endblock %}
		{% include '_partials/footer.html.twig' %}
		{% include '_partials/scripts.html.twig' %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
{% block importmap %}
			{{ importmap('app') }}
		{% endblock %}
		{% endblock %}
	</body>
</html>
