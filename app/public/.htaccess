<IfModule mod_rewrite.c>
    RewriteEngine On

    # Rediriger tout le trafic HTTP vers HTTPS (optionnel)
    # RewriteCond %{HTTPS} !=on
    # RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

    # Rediriger toutes les requÃªtes vers index.php
