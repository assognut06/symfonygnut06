<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<!-- Include header -->
		{#   <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
		#}
		{% include '_partials/header.html.twig' %}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		{% include '_partials/analytics.html.twig' %}
	</head>
	<body>
		<!-- Google Tag Manager (noscript) -->
		<noscript>
			<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PQ7V88VV" height="0" width="0" style="display:none;visibility:hidden"></iframe>
		</noscript>
		<!-- End Google Tag Manager (noscript) -->
		<!-- Include navbar -->
		{% include '_partials/navbar.html.twig' %}

		<!-- Include main content -->
		{% include '_partials/main.html.twig' %}

		<!-- Display flash messages -->
		{% for type, messages in app.flashes %}
			{% for message in messages %}
				<div class="alert alert-{{ type }}" role="alert">{{ message }}</div>
			{% endfor %}
		{% endfor %}

		<!-- Body content -->
		{% block body %}{% endblock %}

		<!-- Include footer -->
		{% include '_partials/footer.html.twig' %}

		<!-- Include scripts -->
		{% include '_partials/scripts.html.twig' %}


		{# Include agent AI #}
		{% include '_partials/agent_ai.html.twig' %}

		<!-- Include javascripts -->
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			<script defer data-cfasync='false' src='https://s.clickiocdn.com/t/243913_wv.js'></script>
		{% endblock %}
	</body>
</html>
