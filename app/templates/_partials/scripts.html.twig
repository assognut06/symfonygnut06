<script>
document.addEventListener('DOMContentLoaded', function() {
    // Masquer le spinner une fois que la page est chargée
    document.getElementById('loadingSpinner').style.display = 'none';
    // Afficher le contenu de la page
    document.getElementById('pageContent').style.display = 'block';
});

// Définissez l'URL de l'image par défaut en dehors du gestionnaire d'événements
var defaultLogoUrl = '{{ asset("images/logo_trans-min.png") }}';

document.addEventListener("DOMContentLoaded", function() {
	var images = document.querySelectorAll('.logoAsso');
	images.forEach(function(img) {
		img.onerror = function() {
			// Utilisez la variable pour l'URL de l'image par défaut
			this.src = defaultLogoUrl;
		};
	});
});
</script>
