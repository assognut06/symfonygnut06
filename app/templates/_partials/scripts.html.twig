<script>
// Script pour gérer l'affichage des images de logo d'association
// Définissez l'URL de l'image par défaut en dehors du gestionnaire d'événements
var defaultLogoUrl = '{{ asset("images/logo_trans-min.webp") }}';

document.addEventListener("DOMContentLoaded", function() {
	var images = document.querySelectorAll('.logoAsso');
	images.forEach(function(img) {
		img.onerror = function() {
			// Utilisez la variable pour l'URL de l'image par défaut
			this.src = defaultLogoUrl;
		};
	});
});

</script>
